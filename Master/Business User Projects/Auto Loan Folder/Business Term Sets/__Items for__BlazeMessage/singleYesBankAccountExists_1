 <instance ref='isNeededEventRuleVH'>
  <instance ref='__ENTITY_PROPERTIES__VH__'>
  </instance>
  <instance ref='calculationVH'>
   <value>theBlazeMessageBT.isSingleYesBankAccount=false;
anApplicant is some Applicant;
for each Applicant in theBlazeMessageBT.termSetInput.request.application.applicants do
{
   anApplicant = it;
   if(anApplicant.bankDetails  is known ) then
  { 
    for each BankDetails in anApplicant.bankDetails such that (it.bankName is known) do
    {
      	if(at least 1 BankDetails in anApplicant.bankDetails satisfies(it.bankName  is equal to  ( "YESB" or  "YES" or  "YBL" or  "532" ))) then
         {
   			    theBlazeMessageBT.isSingleYesBankAccount=true;
         }
    }
  }
}  </value>
  </instance>
 </instance>
