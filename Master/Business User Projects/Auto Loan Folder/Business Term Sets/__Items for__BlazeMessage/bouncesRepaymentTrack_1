  <instance ref='calculationVH'>
   <value><![CDATA[theBlazeMessageBT.bouncesRepaymentTrack = false;
anApplicant is some Applicant;
for each Applicant in theBlazeMessageBT.termSetInput.request.application.applicants do{
  				anApplicant = it;
  if ( anApplicant.liabilities is known) then {  
  		if(at least 1 Liability in anApplicant.liabilities satisfies(it.typeOfLoanProduct is equal to (ignoring case)("19070502" or "3" or "24" or "19070503" or "19070504" or "19070505") and it.noOfBounces > (2*(math().truncate(it.noOfEMIsPaid)/12))))then{
  			theBlazeMessageBT.bouncesRepaymentTrack =true;
		}
  }
}
]]></value>
