anApplicant is some Applicant;

for each Applicant in theFMABT.termSetInput.request.application.applicants do{
  anApplicant = it;
if( it.applicantType="P" and it.customerSegment="1")then{
            Seg_pri=Seg_pri+1;
      }
  if( it.applicantType<>"P" and it.customerSegment="1")then{
  Seg_Co=Seg_Co+1;     
  }
}
if(theFMABT.termSetInput.request.application.applicants.count>1)then{
  Segments=Seg_Co+Seg_pri;
}else{
  Segments=Seg_pri;
}
print("Seg_pri=="Seg_pri);
print("Seg_Co=="Seg_Co);
print("Segments=="Segments);
if(Segments>=1)then{
  theFMABT.SalSegemnt=true;
}
print("SalSegment==" theFMABT.SalSegemnt);]]></value>
