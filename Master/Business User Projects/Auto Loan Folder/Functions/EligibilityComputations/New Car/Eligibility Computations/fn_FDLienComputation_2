 <instance ref='FunctionBodyVH'>
  <value><![CDATA[anApplication is some Application initially blazeMessage.request.application;
anApplicant is some Applicant;
totalFixedDeposit is a real initially 0;

if(anApplication.applicants is known and anApplication.applicants.count > 0)then{
  for each Applicant in anApplication.applicants do 
  {
    anApplicant=it;
    if(anApplicant.FDDetails is known and anApplicant.FDDetails.count > 0) then {
    	for each FDDetails in anApplicant.FDDetails such that(it.amount is known) do 
      	{
    		  totalFixedDeposit +=it.amount;			    
      	}
    }
  }
}

theBlazeMessageBT.maxEligibleAmount = totalFixedDeposit*2;]]></value>
