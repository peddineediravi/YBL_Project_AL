  <value><![CDATA[anApplicant is some Applicant;
score is some Score initially a Score;
isPrimarConstitution is a boolean initially false;

score = blazeMessage.response.applicationDecision.scorecardResult[0];


  for each Applicant in blazeMessage.request.application.applicants such that (it.applicantType is known ) do
{
	anApplicant =it;	
	if(it.applicantType="P"and it.constitution is (known and available) and it.constitution="1" )then
	{
		isPrimarConstitution=true;
	}
}	
print("fn red color coding ");
//red
if (blazeMessage.request.application.schemeGroup <>("6" and "21" and "7" and "8" and "41") and  
	score.scorecardScore<=662 and isPrimarConstitution=true and 
	blazeMessage.request.application.loanDetails.loanAmount is (known and available) and
	blazeMessage.request.application.loanDetails.loanAmount<= 2500000 )then{
print("RED inside ");
blazeMessage.response.applicationDecision.responseCode="RED";
}]]></value>
