  <value><![CDATA[anApplicant is some Applicant;
score is some Score initially a Score;
ispriamaryApplicantIndividual is a boolean initially false;

score = blazeMessage.response.applicationDecision.scorecardResult[0];


  for each Applicant in blazeMessage.request.application.applicants such that (it.applicantType is known ) do
{
	anApplicant =it;	
	
	if(it.applicantType="P"and it.constitution is (known and available) and it.constitution ="1" )then	{
   		ispriamaryApplicantIndividual =true
 }

}	

if(ispriamaryApplicantIndividual =true and blazeMessage.request.application.loanDetails.loanAmount <=2500000 and 	score.scorecardScore>662 and score.scorecardScore <701 
)then{
  print("3rd Amber Condition");
    blazeMessage.response.applicationDecision.responseCode="AMBER";
 
}
 print("Exit fn_colorCodingForSTPAmber===");]]></value>
