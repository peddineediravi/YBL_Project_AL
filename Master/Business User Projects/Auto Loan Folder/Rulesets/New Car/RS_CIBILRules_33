 <instance ref='initializationRule'>
  <instance ref='__ENTITY_PROPERTIES__VH__'>
  </instance>
  <instance ref='initializationAction'>
   <value><![CDATA[creditOrdebitTransactionFlag = fn_NoOfCreditsorDebitsForBankStmtXmonths(blazeMessage);
creditOrdebitTransactionFlag1=fn_NoOfCreditsorDebitsForBankStmtXmonths1(blazeMessage);

print("creditOrdebitTransactionFlag===="creditOrdebitTransactionFlag);
dpdFor3Months is an integer initially 0;
dpdFor6Months is an integer initially 0;
dpdFor12Months is an integer initially 0;
dpdFor24Months is an integer initially 0;
countOfOD is an integer initially 0;
//cumulativeOD is an real initially 0.0;
anApplicant is some Applicant;
datetobeConsidered is a date initially calendar().currentDate();
//suitFilledWriteOffFlag is a boolean initially false;
//paymentHistory is an string initially "";

dpdFor3Months = fn_DPDforOtherThanGurantor(blazeMessage, 3);
if(dpdFor3Months > 0 and dpdFor3Months <= 30) then { dpd3MonthsLE30 = true;}

dpdFor6Months = fn_DPDforOtherThanGurantor(blazeMessage, 6);
if(dpdFor6Months > 30 and dpdFor6Months <=60) then { dpd6MonthsGThan30 = true; }

dpdFor12Months = fn_DPDforOtherThanGurantor(blazeMessage, 12);
if(dpdFor12Months > 60 and dpdFor12Months <= 90) then { dpd12MonthsGThan60 = true; }

dpdFor24Months = fn_DPDforOtherThanGurantor(blazeMessage, 24);
if( dpdFor24Months > 90 ) then { dpd24MonthsGThan90 = true; }


for each Applicant in blazeMessage.request.application.applicants do {
	anApplicant =it;	
	if(anApplicant.multiBureauReport is known and anApplicant.multiBureauReport.cibilResponse is known and anApplicant.multiBureauReport.cibilResponse.accounts is known and anApplicant.multiBureauReport.cibilResponse.accounts.count > 0) then {
		for each Account in anApplicant.multiBureauReport.cibilResponse.accounts such that (it.owenershipIndicator is known and it.owenershipIndicator is different from "GUARANTOR" )do {
		if(it.dateClosed is (known and available)  ) then {
			datetobeConsidered = it.dateClosed;
			}else if (it.dateOfLastPayment is (known and available)) then {
				datetobeConsidered = it.dateOfLastPayment;
			}else if (it.dateReportedAndCertified is (known and available)  ) then { 
			datetobeConsidered = it.dateReportedAndCertified;
			}
          		if (datetobeConsidered is known and (calendar().currentDate().subtractInMonths(datetobeConsidered).months >= 0) and (calendar().currentDate().subtractInMonths(datetobeConsidered).months <= 24) )then  {
                  if (it.amountOverdue is known and it.amountOverdue > 10000) then {
                  				countOfOD = countOfOD +1;
                	}
                  	if (it.amountOverdue is known and it.amountOverdue >= 1) then {
                  				cumulativeOD = cumulativeOD +it.amountOverdue;
                	}
                  if( it.suitFiledOrWilfulDefault is (known and available) and it.suitFiledOrWilfulDefault is not null and it.suitFiledOrWilfulDefault <> "" )then {
                    	suitFilledWriteOffFlag = true;
                  }
                  if(it.writtenOffAndSettled is (known and available) and it.writtenOffAndSettled >= 0) then {
                    	writtenOffAndSettledFlag = true;
                  }
                }
        }
    }
}
if(countOfOD >=1) then {
  odAmountGThan10K = true;
}

print("suitFilledWriteOffFlag="suitFilledWriteOffFlag);
print("dpd3MonthsLE30="dpd3MonthsLE30);
print("dpd6MonthsGThan30="dpd6MonthsGThan30);
print("dpd12MonthsGThan60="dpd12MonthsGThan60);
print("dpd24MonthsGThan90="dpd24MonthsGThan90);
print("odAmountGThan10K="odAmountGThan10K);
print("writtenOffAndSettledFlag="writtenOffAndSettledFlag);
print("cumulativeOD="cumulativeOD);]]></value>
  </instance>
