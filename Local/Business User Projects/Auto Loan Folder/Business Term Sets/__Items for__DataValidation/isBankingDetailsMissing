 <instance ref='isNeededEventRuleVH'>
  <instance ref='__ENTITY_PROPERTIES__VH__'>
  </instance>
  <instance ref='calculationVH'>
   <value>theDataValidation.isBankingDetailsParamsMissing=false;
anApplicant is some Applicant;
for each Applicant in theBlazeMessageBT.termSetInput.request.application.applicants do 
  {
    anApplicant=it;
   if(it.bankDetails is known) then{ 
    for each BankDetails in anApplicant.bankDetails such that (it.isRepaymentAccount is known and it.isRepaymentAccount = true) do{
             if(it.noOfCredits is unknown or it.noOfDebits is unknown or it.noOfInwardChequeReturns is unknown or it.noOfOutwardChequeReturns is unknown) then 
               {
                 theDataValidation.isBankingDetailsParamsMissing=true;
               }
 		   }
 	 	 }
 	 }
  </value>
  </instance>
 </instance>
